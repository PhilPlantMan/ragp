Package: ragp
Type: Package
Title: Mining for Hydroxyproline rich glycoprotein sequences
Version: 0.0.0.9007
Authors@R: c(
  person("Milan", "Dragićević", email = "mdragicevic@ibiss.bg.ac.rs",
         role = c("aut", "cre")),
  person("Danijela", "Paunović", role = "aut"),
